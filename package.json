{
  "name": "prompt-expand",
  "description": "Expand prompt. Can be used as a standalone prompt, or with a prompt system like [Enquirer].",
  "version": "0.1.0",
  "homepage": "https://github.com/enquirer/prompt-expand",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "enquirer/prompt-expand",
  "bugs": {
    "url": "https://github.com/enquirer/prompt-expand/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "log-utils": "^0.2.1",
    "prompt-base": "^0.5.0",
    "terminal-paginator": "^0.2.1"
  },
  "devDependencies": {
    "enquirer": "^0.3.1",
    "gulp-format-md": "^0.1.11"
  },
  "keywords": [
    "answer",
    "answers",
    "ask",
    "checkbox",
    "choice",
    "cli",
    "command",
    "enquirer",
    "enquirerprompt",
    "expand",
    "input",
    "inquire",
    "inquirer",
    "interact",
    "list",
    "menu",
    "password",
    "prompt",
    "prompts",
    "question",
    "readline",
    "stdin",
    "stdout",
    "terminal",
    "tty",
    "ui"
  ],
  "verb": {
    "toc": false,
    "layout": "common-minimal",
    "data": {
      "alias": "expand"
    },
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "enquirer",
        "prompt-base",
        "prompt-checkbox",
        "prompt-confirm",
        "prompt-radio"
      ]
    },
    "reflinks": [
      "enquirer",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}